<h4 class="text-capitalize">master jenis buku</h4>

<div class="row mt-4">
    <div class="col-lg-4">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>kode</th>
                    <th>nama</th>
                </tr>
            </thead>
            <tbody>
                <% for( let i=0; i < jenis_buku.length; i++) { %>
                    <tr>
                        <td><%= jenis_buku[i].kode %></td>
                        <td><%= jenis_buku[i].nama %></td>
                    </tr>
                <% } %>
            </tbody>
        </table>
    </div>
</div>