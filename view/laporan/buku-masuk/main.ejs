
<h4 class="text-capitalize">laporan buku masuk</h4>

<table id="table_laporan" class="table table-sm table-hover">
    <thead class="table-light">
        <tr>
            <th>Judul</th>
            <th>Penerbit</th>
            <th>Jenis</th>
            <th>Genre</th>
            <th>Pengarang</th>
            <th>Ilustrator</th>
            <th>Jumlah masuk</th>
            <th>Kategori masuk</th>
            <th>Tanggal masuk</th>
        </tr>
    </thead>
    <tbody>
        <% stok_masuk.forEach(sm => { %>
            <tr>
                <td><%= sm.nama %></td>
                <td><%= sm.penerbit %></td>
                <td><%= sm.jenis_nama %></td>
                <td><%= sm.genre_nama %></td>
                <td><%= sm.pengarang %></td>
                <td><%= sm.ilustrator %></td>
                <td><%= sm.jumlah_masuk %></td>
                <td><%= sm.kategori %></td>
                <td><%= moment(sm.created_at).format('DD MMMM YYYY HH:mm') %></td>
            </tr>
        <% }) %>
    </tbody>
</table>

<link rel="stylesheet" href="/datatables/css/buttons.dataTables.css">
<script src="/datatables/js/dataTables.buttons.js"></script>
<script src="/datatables/js/buttons.dataTables.js"></script>
<script src="/jszip/jszip.min.js"></script>
<script src="/pdfmake/pdfmake.min.js"></script>
<script src="/pdfmake/vfs_fonts.js"></script>
<script src="/datatables/js/buttons.html5.min.js"></script>
<script src="/datatables/js/buttons.print.min.js"></script>

<script>
    new DataTable('#table_laporan', {
        layout: {
            topStart: {
                buttons: ['copy', 'csv', 'excel', 'pdf', 'print']
            }
        }
    })
</script>
